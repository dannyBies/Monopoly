        <KeyBinding Command="{Binding ThrowDiceCommand }" Key="t"/>
        <KeyBinding Command="{Binding BuyCommand }" Key="b"/>
        <KeyBinding Command="{Binding ManageCommand }" Key="m"/>
        <KeyBinding Command="{Binding EndTurnCommand }" Key="e"/>
    </Window.InputBindings>

